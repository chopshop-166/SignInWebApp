{% extends "base.html.jinja2" %}
{% from 'bootstrap5/utils.html' import render_static, render_icon %}

{% block title %}Subteam {{subteam.name}} - Chop Shop Sign In{% endblock %}

{% block content %}
<div class="container">
  <h1>{{subteam.name}}</h1>
  <div class="row">
    <div class="col">
      <div>
        <ul>
          {%- for member in subteam.members -%}
            <li><a href="{{url_for('user.profile', user_id=member.id)}}">{{member.name}}</a></li>
          {%- endfor -%}
        </ul>
      </div>
    </div>
  </div>
</div>
{% endblock %}