{% extends "base.html.jinja2" %}
{% from 'bootstrap5/utils.html' import render_static, render_icon %}
{% block title %}
  Subteam {{ subteam.name }}
{% endblock title %}
{% block content %}
  <div class="container">
    <h1>{{ subteam.name }}</h1>
    <div class="row">
      <div class="col">
        <div>
          <ul>
            {%- for member in subteam.members -%}
              <li>
                <a href="{{ url_for('user.profile', username=member.username)}}">{{ member.name }}</a>
              </li>
            {%- endfor -%}
          </ul>
        </div>
      </div>
    </div>
  </div>
{% endblock content %}
